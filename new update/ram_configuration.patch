From f5f0e97db69a735b75c9a33fc9aa6c01a42703a2 Mon Sep 17 00:00:00 2001
From: Mathanraj <mathan.raj@seco.com>
Date: Fri, 23 Jun 2017 10:58:45 +0530
Subject: [PATCH] RAM configuration for 984

---
 board/seco/ddr/mx6_SBC_984_ram.cfg | 91 ++++++++++++++++++++++++++++++++++++++
 1 file changed, 91 insertions(+)
 create mode 100755 board/seco/ddr/mx6_SBC_984_ram.cfg

diff --git a/board/seco/ddr/mx6_SBC_984_ram.cfg b/board/seco/ddr/mx6_SBC_984_ram.cfg
new file mode 100755
index 0000000..5551c8d
--- /dev/null
+++ b/board/seco/ddr/mx6_SBC_984_ram.cfg
@@ -0,0 +1,91 @@
+/*
+ * Copyright (C) 2013 Seco USA Inc
+ *
+ * SPDX-License-Identifier:     GPL-2.0+
+ *
+ * Refer doc/README.imximage for more details about how-to configure
+ * and create imximage boot image
+ *
+ * The syntax is taken as close as possible with the kwbimage
+ */
+
+/* image version */
+IMAGE_VERSION 2
+
+/*
+ * Boot Device : one of
+ * spi, sd (the board has no nand neither onenand)
+ */
+BOOT_FROM      sd
+
+
+#define __ASSEMBLY__
+#include <config.h>
+#include "asm/arch/mx6-ddr.h"
+#include "asm/arch/iomux.h"
+#include "asm/arch/crm_regs.h"
+
+#if defined CONFIG_SECOMX6Q || defined CONFIG_SECOMX6DL || defined CONFIG_SECOMX6S
+#include "clocks.cfg"
+#endif
+
+#ifdef CONFIG_SECOMX6Q
+
+	#if defined ( CONFIG_SECOMX6_512MB_2x256 )
+		#include "quad/ddr_bit_32_qd.cfg"
+		#include "cfg_984/984QD_2x256M.cfg" 
+
+	#elif defined( CONFIG_SECOMX6_1GB_2x512 )
+
+	#elif defined ( CONFIG_SECOMX6_1GB_4x256 )
+
+		#include "quad/ddr_bit_64_qd.cfg"
+		#include "cfg_984/984QD_4x256M.cfg"
+
+	#elif defined ( CONFIG_SECOMX6_2GB_4x512 )
+		#include "quad/ddr_bit_64_qd.cfg"
+		#include "cfg_984/984QD_4x512M.cfg"
+
+	#elif defined( CONFIG_SECOMX6_4GB_8x512 )
+
+	#endif
+
+#elif CONFIG_SECOMX6DL
+
+	#if defined ( CONFIG_SECOMX6_512MB_2x256 )
+		#include "dual_lite/ddr_bit_32_dl.cfg"
+		#include "cfg_984/984DL_2x256M.cfg"
+
+	#elif defined( CONFIG_SECOMX6_1GB_2x512 )
+
+	#elif defined( CONFIG_SECOMX6_1GB_4x256 )
+		#include "dual_lite/ddr_bit_64_dl.cfg"
+		#include "cfg_984/984DL_4x256M.cfg"
+
+	#elif defined( CONFIG_SECOMX6_2GB_4x512 )
+		#include "dual_lite/ddr_bit_64_dl.cfg"
+		#include "cfg_984/984DL_4x512M.cfg"
+
+	#elif defined( CONFIG_SECOMX6_4GB_8x512 )
+
+	#endif
+
+#elif CONFIG_SECOMX6S
+
+	#if defined( CONFIG_SECOMX6_512MB_2x256 )
+		#include "solo/ddr_bit_32_solo.cfg"
+		#include "cfg_984/984SOLO_2x256M.cfg"
+
+	#elif defined( CONFIG_SECOMX6_1GB_2x512 )
+		#include "solo/ddr_bit_32_solo.cfg"
+		#include "cfg_984/984SOLO_2x512M.cfg"
+
+	#elif defined( CONFIG_SECOMX6_1GB_4x256 )
+
+	#elif defined( CONFIG_SECOMX6_2GB_4x512 )
+
+	#elif defined( CONFIG_SECOMX6_4GB_8x512 )
+
+	#endif
+
+#endif
-- 
